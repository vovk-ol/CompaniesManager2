<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://vovk.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>vovk.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>vovk</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$vovk</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="TreeViewModel" Order="1" Enabled="True">
          <Destination Path="Data Source=tcp:vovk.database.windows.net,1433;Initial Catalog=vovkDB;User ID=vovk@vovk;Password={your_password_here};Connect Timeout=30;Encrypt=True;TrustServerCertificate=False" Name="Server=tcp:vovk.database.windows.net,1433;Database=vovkDB;User ID=vovk@vovk;Password={your_password_here};Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="CompaniesManager2.Models.TreeViewModel, CompaniesManager2" MigrationConfiguration="CompaniesManager2.Migrations.Configuration, CompaniesManager2" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)TreeViewModel-Web.config Connection String">
      <ParameterValue>Server=tcp:vovk.database.windows.net,1433;Database=vovkDB;User ID=vovk@vovk;Password={your_password_here};Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>